<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk.Web">
  <Target Name="ShowVars" BeforeTargets="Restore">
    <Message Importance="High" Text="PaketToolsPath = $(PaketToolsPath)" />
    <Message Importance="High" Text="PaketBootstrapperStyle = $(PaketBootstrapperStyle)" />
    <Message Importance="High" Text="PaketExeImage = $(PaketExeImage)" />
    <Message Importance="High" Text="PaketBootstrapperExePath = $(PaketBootstrapperExePath)" />
    <Message Importance="High" Text="PaketExePath = $(PaketExePath)" />
    <Message Importance="High" Text="PaketBoostrapperCommand = $(PaketBoostrapperCommand)" />
  </Target>
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Startup.fs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Bolero.Template.Client\Bolero.Template.Client.fsproj" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>